{
  "name": "client-configs",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "setup": "eth-sdk",
    "clean": "rimraf build",
    "apply": "tsc && node build/scripts/apply.js",
    "apply:export": "tsc && node build/scripts/applyExport.js",
    "anvil": "anvil --silent --fork-url ${FORK_RPC:-https://rpc.ankr.com/eth}",
    "test:watch-only": "jest --runInBand --watch",
    "test": "concurrently --raw --kill-others --passthrough-arguments --success first \"yarn anvil\" \"jest --runInBand {@}\" --",
    "test:watch": "concurrently --raw --kill-others --handle-input --passthrough-arguments \"yarn anvil\" \"jest --runInBand --watch {@}\" --",
    "check:types": "tsc --noEmit",
    "check:prettier": "prettier '**/*.{js,ts,tsx,json,md}' --list-different",
    "fix:prettier": "yarn prettier '**/*.{js,ts,tsx,json,md}' --write"
  },
  "dependencies": {
    "@gnosis-guild/eth-sdk": "^0.3.7",
    "@gnosis-guild/eth-sdk-client": "^0.1.6",
    "@gnosis-guild/zodiac": "^4.0.3",
    "@types/jest": "^29.5.2",
    "@types/wait-on": "^5.3.1",
    "concurrently": "^8.2.1",
    "defi-kit": "^2.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.3",
    "wait-on": "^7.0.1",
    "yargs": "^17.7.2",
    "zodiac-roles-sdk": "^2.14.5"
  },
  "packageManager": "yarn@1.22.19+sha512.ff4579ab459bb25aa7c0ff75b62acebe576f6084b36aa842971cf250a5d8c6cd3bc9420b22ce63c7f93a0857bc6ef29291db39c3e7a23aab5adfd5a4dd6c5d71",
  "devDependencies": {
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  },
  "lint-staged": {  
    "*.ts": [  
      "yarn prettier --write ."
    ]  
  }  
}
